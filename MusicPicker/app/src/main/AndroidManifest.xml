<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicpicker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CategoriesActivity"
            android:label="Categories" />
        <activity
            android:name=".MenuActivity"
            android:label="Menu" />
        <activity
            android:name=".RomanticSongs"
            android:label="@string/romanticSongs" />
        <activity
            android:name=".sadsongs"
            android:label="@string/sadSongs" />
        <activity
            android:name=".trancesongs"
            android:label="@string/tranceSongs" />
        <activity
            android:name=".hiphopsongs"
            android:label="@string/hiphopSongs" />
        <activity
            android:name=".workoutsongs"
            android:label="@string/workoutSongs" />
        <activity
            android:name=".jazzsongs"
            android:label="@string/jazzSongs" />
        <activity
            android:name=".PaymentActivity"
            android:label="Buy" />
    </application>

</manifest>